<%= simple_form_for [@loutre, @location] do |f| %>
  <%= f.input :date_arrive, label: "Date de début", as: :date, start_year: Date.today.year, date_arrive: Date.today + 1, end_year: Date.today.year + 5, order: [:day, :month, :year] %>

  <%= f.input :date_départ, label: "Date de fin" , as: :date, start_year: Date.today.year, date_départ:
  Date.today.year + 5, order: [:day, :month, :year] %>
  <%= f.input :address %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
